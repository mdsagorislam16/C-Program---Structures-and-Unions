#include <stdio.h>
#include <string.h>

struct Student {
    char name[50];
    int roll;
};

int main() {
    struct Student s[3];
    for(int i=0;i<3;i++) {
        fgets(s[i].name, 50, stdin);
        s[i].name[strcspn(s[i].name,"\n")] = '\0';
        scanf("%d",&s[i].roll);
        getchar();
    }

    // Sort by name
    for(int i=0;i<3-1;i++) {
        for(int j=i+1;j<3;j++) {
            if(strcmp(s[i].name,s[j].name)>0) {
                struct Student temp = s[i];
                s[i] = s[j];
                s[j] = temp;
            }
        }
    }

    for(int i=0;i<3;i++)
        printf("%s %d\n", s[i].name, s[i].roll);

    return 0;
}

/*
Input:
John
101
Alice
102
Bob
103

Output:
Alice 102
Bob 103
John 101
*/
